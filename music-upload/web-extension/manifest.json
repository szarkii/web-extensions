{
  "manifest_version": 2,
  "name": "Music Upload",
  "version": "1.1",
  "description": "Sends request to upload music.",
  "icons": {
    "512": "icons/icon-512.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{c40533c9-38ae-4116-811b-89254263bf42}"
    }
  },
  "options_ui": {
    "page": "settings/settings.html"
  },
  "browser_action": {
    "default_icon": "icons/icon-512.png",
    "default_title": "Upload",
    "default_popup": "popup/popup.html"
  }
}